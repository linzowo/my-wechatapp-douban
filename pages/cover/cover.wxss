page{
    height: 100%;    
}

.container{
    height: 100%;
}

swiper{
    height: 100%;
}
swiper swiper-item {
  display: flex;
  align-items: center;
  justify-content: center; 
}
swiper image{
    width: 100%;
    height: 100%;
    position: absolute;
}
swiper swiper-item button {
    position: absolute;
    bottom: 200rpx;
    width: 300rpx;
    font-size: 40rpx;
    border-radius: 200rpx;
    border: 1px solid rgba(255,255,255,.8);
    background-color: rgba(255,255,255,.6);
}
